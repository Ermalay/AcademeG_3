<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.novil.sergey.navigationdraweractivity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="ru.novil.sergey.academegtruestories.other.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon_academeg"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="ru.novil.sergey.academegtruestories.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="ru.novil.sergey.academegtruestories.Delete_It"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name="ru.novil.sergey.academegtruestories.view.SplashScreen"
            android:theme="@style/SplashTheme" />
        <activity
            android:name="ru.novil.sergey.academegtruestories.YTActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="sensor"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
        <activity android:name="ru.novil.sergey.academegtruestories.ThirdFragment" />
        <activity android:name="ru.novil.sergey.academegtruestories.FireBaseTest" />
        <activity
            android:name="ru.novil.sergey.academegtruestories.view.SplashScreenN"
            android:theme="@style/SplashTheme" />
        <activity
            android:name="ru.novil.sergey.academegtruestories.view.SplashScreenNN"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>